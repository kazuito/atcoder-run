{
  "name": "atcoder-run",
  "displayName": "AtCoder Run",
  "description": "Enhance your AtCoder experience.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "license": "MIT",
  "publisher": "kazuito",
  "repository": {
    "type": "git",
    "url": "https://github.com/kazuito/atcoder-run.git"
  },
  "bugs": {
    "url": "https://github.com/kazuito/atcoder-run/issues"
  },
  "keywords": [
    "atcoder"
  ],
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "panel": [
        {
          "id": "AtCoderRun",
          "title": "AtCoder Run",
          "icon": "$(run)"
        }
      ]
    },
    "views": {
      "AtCoderRun": [
        {
          "id": "atcoder-run.views.result",
          "name": "Result",
          "type": "webview"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "atcoder-run.runTest",
          "when": "view == atcoder-run.views.result",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "atcoder-run.login",
        "category": "AtCoder",
        "title": "Login"
      },
      {
        "command": "atcoder-run.logout",
        "category": "AtCoder",
        "title": "Logout"
      },
      {
        "command": "atcoder-run.runTest",
        "category": "AtCoder",
        "title": "Run Test",
        "icon": "$(run)"
      }
    ],
    "configuration": {
      "title": "AtCoder Run",
      "properties": {
        "atcoder-run.showInputForPassed": {
          "type": "boolean",
          "default": false
        },
        "atcoder-run.showOutputFromPassed": {
          "type": "boolean",
          "default": true
        },
        "atcoder-run.showInputForFailed": {
          "type": "boolean",
          "default": true
        },
        "atcoder-run.autoCopy": {
          "type": "boolean",
          "default": true
        },
        "atcoder-run.notifyOnAutoCopy": {
          "type": "boolean",
          "default": true
        }
      }
    },
    "languages": [
      {
        "id": "atcoder-run"
      }
    ],
    "grammars": [
      {
        "language": "atcoder-run",
        "scopeName": "source.atcoder-run",
        "path": "./syntaxes/atcoder-run.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "yaml2json": "npx js-yaml syntaxes/atcoder-run.tmLanguage.yml > syntaxes/atcoder-run.tmLanguage.json"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "js-yaml": "^4.1.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4",
    "yq": "^0.0.1"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/superagent": "^4.1.16",
    "cheerio": "^1.0.0-rc.12",
    "superagent": "^8.0.9",
    "vite-plugin-css-injected-by-js": "^3.0.1"
  }
}
